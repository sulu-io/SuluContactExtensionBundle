    "version": "dev-develop",
    "keywords": ["contact", "crm"],
    "require": {
        "php": ">=5.4",
        "doctrine/orm": "2.4.*",
        "willdurand/hateoas-bundle": "0.3.*",

        "oro/doctrine-extensions": "1.0.*",
